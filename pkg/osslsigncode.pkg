_pkg=osslsigncode
_version=2.6.0
_uri=https://github.com/mtrojnar/osslsigncode/releases/download/2.6/osslsigncode-2.6.tar.gz
_dep="curl openssl-1.1.1 libssh2"
_build_dep="compiletc coreutils cmake openssl-1.1.1-dev curl-dev libssh2-dev python3.9"
_pkg_path=osslsigncode-2.6
_pre_config="sudo -u tc mkdir -p $builddir/$_pkg_path/build"
_conf_cmd="cd build; setarch linux$arch cmake -S .."
_make_cmd="cd build; cmake --build ."
_install_cmd="cd build; make install DESTDIR=" 
_pkgs='main{/usr}'
